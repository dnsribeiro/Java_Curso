package Exercicio917;

public class Conta {

	private int numero;
	private String agencia;

	public Conta() {
		this.numero = 0;
		this.agencia = "";
	}

	public int getNumero() {
		return numero;
	}

	public void setNumero(int numero) {
		this.numero = numero;
	}

	public String getAgencia() {
		return agencia;
	}

	public void setAgencia(String agencia) {
		this.agencia = agencia;
	}

	
}
