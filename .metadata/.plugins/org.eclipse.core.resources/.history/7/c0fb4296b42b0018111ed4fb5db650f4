package Exercicio412;

public class Fibonacci {
	
	public int calcular (int _valor) {
		if (_valor == 1)
			return (1);
		else {
			return _valor * calcular(_valor - 1);
		}
	}
	
	//Exercício 03 (Desafio)...
	public int calcularTernario (int _valor) {
		return _valor == 1 ? 1 : _valor * calcular(_valor - 1);
	}	

}
