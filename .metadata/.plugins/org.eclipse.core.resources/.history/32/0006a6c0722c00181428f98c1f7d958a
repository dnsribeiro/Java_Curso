package Desafio138;

public class ConversaoStr {

	public static void main(String[] args) {
		
		String s1 = "4587";
		int num = 0;
		
		for (int i=s1.length()-1; i>=0; i--) {
			char c = s1.charAt(i);
			int aux = c - '0';
			
			num += num + (aux * (10 * s1.length() - i));
		}
		
		System.out.println(num+10);
	}
	
}
